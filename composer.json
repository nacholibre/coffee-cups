{
    "name": "nacholibre/coffee-cups",
    "description": "A simple PHP library for CUPS IPP (Internet Printing Protocol) communication",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cups",
        "ipp",
        "printing",
        "print",
        "printer"
    ],
    "authors": [
        {
            "name": "Yoan Arnaudov"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CoffeeCups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoffeeCups\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

